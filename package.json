    "chartjs-adapter-date-fns": "^3.0.0",
    "cmdk": "^1.0.0",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "duckdb-async": "^1.1.3",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.3",
