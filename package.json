{
  "name": "apex-algorithms-catalogue-web",
  "version": "0.24.2",
  "type": "module",
  "scripts": {
    "setup": "make-dir ./tmp && npm run download-source && npm run generate-acl && astro check",
    "dev": "npm run setup && astro dev --force",
    "build": "npm run setup && astro build",
    "build-test": "npm run setup && node ./scripts/setup-tests.mjs && astro build && node ./scripts/post-setup-tests.mjs",
    "preview": "astro preview",
    "astro": "astro",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "test": "npm run test-unit && npm run test-e2e",
    "test-unit": "vitest run",
    "test-e2e": "npm run build-test && playwright test",
    "test-e2e-ui": "npm run build-test && playwright test --ui",
    "download-source": "node ./scripts/download-source.mjs",
    "download-navbar-footer": "node ./scripts/download-navbar-footer.mjs",
    "generate-acl": "node ./scripts/generate-acl.mjs"
  },
  "dependencies": {
    "@astrojs/node": "^8.3.3",
    "@auth/core": "^0.37.4",
    "astro": "^4.14.2",
    "auth-astro": "^4.2.0",
